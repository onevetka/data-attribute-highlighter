{
  "manifest_version": 2,
  "name": "Data Attribute Highlighter",
  "version": "1.0.0",
  "description": "Highlights data-* attributes on web page",
  "homepage_url": "http://mikevetkin.com",
  "browser_action": {
		"default_popup": "widgetModal.html"
	},
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["src/content-script.js"],
      "css" : ["src/style.css"]
    }
  ]
}